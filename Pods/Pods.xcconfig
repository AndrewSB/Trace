FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Realm/realm-ios-0.80.0"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/Realm" "${PODS_ROOT}/Headers/SOMotionDetector"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/Realm" -isystem "${PODS_ROOT}/Headers/SOMotionDetector"
OTHER_LDFLAGS = -ObjC -lc++ -framework CoreLocation -framework CoreMotion -framework Realm
PODS_ROOT = ${SRCROOT}/Pods